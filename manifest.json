{
  "name": "No Merge Button",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Disable GitLab interactive conflict resolution in testing branch.",
  "homepage_url": "https://codeigniter.org.cn",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["js/content.js"]
		}
	],
  "host_permissions": [
    "*://*/*"
  ]
}
